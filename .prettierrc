{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "singleAttributePerLine": true,
  "printWidth": 100,
  "proseWrap": "always",
  "braketSpacing": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES",
    "^@/components/(.*)$",
    "^@/lib/(.*)$",
    "^@/types$",
    "^@/db/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
